<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
            <meta charset="UTF-8"/>
            <title>{% block title %}Welcome!{% endblock %}</title>
            {% block stylesheets %}
                <link rel="stylesheet" href="{{ asset('assets/bower_components/dist/css/bootstrap.min.css') }}" />
                <link rel="stylesheet" href="{{ asset('css/main.css') }}" />
            {% endblock %}
            <link rel="stylesheet" href="{{ asset('assets/bower_components/bootstrap/dist/css/bootstrap.css') }}" />
            <link rel="stylesheet" href="{{ asset('assets/frontend/css/freelancer.css') }}" />
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
    </head>
    <body>
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}

        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </body>
</html>
